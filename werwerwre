

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Robojini/Tuturial_UI_Library/main/UI_Template_1"))()
local Window = Library.CreateLib("Smilley", "RJTheme3")
local player = game.Players.LocalPlayer
local character = player.Character
local i = 0
local s = Instance.new("Highlight")
local character = player.Character
local SoulsSpawn = workspace:FindFirstChild("SoulsSpawn")
local Tab = Window:NewTab("Main")



local Section = Tab:NewSection("Basic Functions")


Section:NewButton("Esp Enemy","Killing for 1 Second (Have BYpass)", function()
while wait(0.1) do
for i,d in pairs(game.Workspace.System.Enemies:GetChildren()) do
    local p = s:Clone()
	p.Parent = d
	p.FillTransparency = 0.9
end
end
end)


Section:NewSlider("Speed Hack", "Adding Speed To Your Humanoid", 200, 0, function(s) -- 200 (Макс. значение) | 0 (Мин. значение)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

Section:NewToggle("GodMode", "God Mode (Noclip)", function(state)
    if state then
        for _, part in pairs(character:GetChildren()) do
		if part:IsA("Part") or part.Name == "Hitbox" then
			part.CanTouch = false
		end
        end  

    else
        for _, part in pairs(character:GetChildren()) do
    if part:IsA("Part") or part.Name == "Hitbox" then
        part.CanTouch = true
    end
end    
end
end)

Section:NewToggle("No Attacks", "God Mode (Noclip)", function(state)
    if state then
game.Workspace.System.EnemyAttacks.Parent = game.ReplicatedStorage
    else
game.ReplicatedStorage.EnemyAttacks.Parent = game.Workspace.System
	end
end)

Section:NewButton("ShortCut (No CD)", "Auto collect souls", function()
mouse = game.Players.LocalPlayer:GetMouse()
tool = Instance.new("Tool")
tool.RequiresHandle = false
tool.Name = "ShortCut"
tool.Activated:connect(function()
local pos = mouse.Hit+Vector3.new(0,2.5,0)
pos = CFrame.new(pos.X,pos.Y,pos.Z)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
end)
tool.Parent = game.Players.LocalPlayer.Backpack
end)

Section:NewButton("Fast Start", "Auto collect souls", function()
game.Workspace:FindFirstChild("Start").Size = Vector3.new(1, 1, 1)
game.Workspace:FindFirstChild("Start").CFrame = player.Character.HumanoidRootPart.CFrame
wait(0.00000000001)
game.Workspace:FindFirstChild("Start").CFrame = CFrame.new(9989.8828125, 672.8535766601562, 1920.8428955078125)
game.Workspace:FindFirstChild("Start").Size = Vector3.new(1000, 15, 15)
end)



local Tab = Window:NewTab("Locations Teleport")



local Section = Tab:NewSection("Basic Functions")

Section:NewButton("Area","Killing for 1 Second (Have BYpass)", function()
player.Character:PivotTo(CFrame.new(9989.814453125, 327.8664855957031, 2168.752685546875))
end)

Section:NewButton("lobby","Killing for 1 Second (Have BYpass)", function()
player.Character:PivotTo(CFrame.new(21.011157989501953, 212.97463989257812, -986.6199340820312))
end)

Section:NewButton("Admin Room","Killing for 1 Second (Have BYpass)", function()
game.Workspace.RegularLobby.AdminRoom.Detect.Disabled = true
game.Workspace.RegularLobby.AdminRoom.Detect:Destroy()
player.Character:PivotTo(CFrame.new(-5172.24365234375, 212.97462463378906, -2559.025146484375))
end)





